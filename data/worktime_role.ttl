                        wtr:r2.16 , # civil servant
                        wtr:r2.17 , # technician
                        wtr:r2.18 , # "simple" pension fund member
                        wtr:r2.19 , # conscript
                        wtr:r2.20 . # Vermindert Erwerbsfähige

