wtt:t0.1.1 a skos:Concept ;        # Workday
    skos:inScheme       wtt:scheme ;
    skos:notation       "t0.1.1" ;
    skos:prefLabel      "Arbeitstag"@de ;
    skos:narrower       wtt:t0.1.1.1 ,
                        wtt:t0.1.1.2 ;
